{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, must-revalidate" },
        { "key": "CDN-Cache-Control", "value": "max-age=0" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/en.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/jp.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/jp",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}