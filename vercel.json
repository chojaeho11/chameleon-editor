{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/en.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/jp.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/jp",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}